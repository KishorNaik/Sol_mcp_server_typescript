{
	"name": "@kishornaik/todo_mcp_server",
	"version": "1.0.0",
	"description": "",
	"main": "dist/core/index.js",
	"type": "module",
	"scripts": {
		"start": "npm run build && node dist/core/index.js",
		"start:local": "ts-node-dev --respawn --pretty --transpile-only ./src/core/index.ts",
		"debug": "ts-node-dev --inspect=4321 --pretty --transpile-only ./src/core/index.ts",
		"clean": "rimraf dist",
		"build:dev": "npm run clean && npm run build:tsc && npm run prettier && swc src -d dist --source-maps --copy-files",
		"build": "npm run clean && npm run build:tsc && npm run prettier && swc src -d dist --source-maps --copy-files",
		"build:tsc": "tsc -p tsconfig.build.json",
		"build:watch": "tsc -w -p tsconfig.build.json",
		"lint": "eslint . --ext .ts --ext .mts",
		"test": "glob -c \"node --trace-deprecation --no-warnings --test --require ts-node/register -r tsconfig-paths/register\" \"./src/test/**/*.test.ts\"",
		"prettier": "prettier --config .prettierrc --write .",
		"prepare": "husky",
		"npm:outdated": "npm outdated",
		"npm:latest-package-update": "npx npm-check-updates -u",
		"mcp-inspector": "npx fastmcp inspect ./src/core/index.ts",
    "mcp-cli":"npx fastmcp dev ./src/core/index.ts"
	},
	"author": "",
	"license": "ISC",
	"engines": {
		"node": ">= 14 < 21"
	},
	"devDependencies": {
		"@swc/cli": "^0.6.0",
		"@swc/core": "^1.11.9",
		"@types/glob": "^8.1.0",
		"@types/ioredis": "^4.28.10",
		"@types/node": "^22.13.10",
		"@types/sinon": "^17.0.4",
		"expect": "^29.7.0",
		"glob": "^11.0.1",
		"husky": "^9.1.7",
		"prettier": "^3.5.3",
		"rimraf": "^6.0.1",
		"sinon": "^19.0.2",
		"standard-version": "^9.5.0",
		"ts-api-utils": "^2.0.1",
		"ts-node": "^10.9.2",
		"ts-node-dev": "^2.0.0",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.8.2"
	},
	"dependencies": {
		"@faker-js/faker": "^9.6.0",
		"@types/amqplib": "^0.10.7",
		"@types/opossum": "^8.1.8",
		"amqp-connection-manager": "^4.1.14",
		"amqplib": "^0.10.5",
		"axios": "^1.8.3",
		"bullmq": "^5.49.1",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.1",
		"dotenv": "^16.4.7",
		"envalid": "^8.0.0",
		"fastmcp": "^1.27.6",
		"guid-typescript": "^1.0.9",
		"http-status-codes": "^2.3.0",
		"ioredis": "^5.6.1",
		"linq": "^3.2.4",
		"mediatr-ts": "^2.0.1",
		"neverthrow": "^8.2.0",
		"openai": "^4.87.3",
		"opossum": "^8.4.0",
		"readline-sync": "^1.4.10",
		"redis": "^4.7.0",
		"reflect-metadata": "^0.2.2",
		"tsx": "^4.19.4",
		"typedi": "^0.10.0",
		"typeorm": "^0.3.21",
		"uuid": "^11.1.0",
		"winston": "^3.17.0",
		"winston-daily-rotate-file": "^5.0.0",
		"zod": "^3.24.4"
	},
	"files": [
		"dist"
	]
}
